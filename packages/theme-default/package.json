{
	"name": "@sylphx/leaf-theme-default",
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/src/index.js",
	"types": "./dist/src/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/src/index.js",
			"types": "./dist/src/index.d.ts"
		},
		"./style.css": "./dist/style.css"
	},
	"files": ["dist"],
	"scripts": {
		"build": "bunup 'src/**/*.{ts,tsx}' && cp src/styles/index.css dist/style.css && node ../../scripts/fix-exports.js",
		"dev": "bunup 'src/**/*.{ts,tsx}' --watch"
	},
	"dependencies": {
		"@sylphx/silk": "^1.1.0",
		"@sylphx/zen": "^1.2.1",
		"katex": "^0.16.11",
		"minisearch": "^7.2.0",
		"react-router-dom": "^7.0.1"
	},
	"devDependencies": {
		"@types/react": "^18.3.12",
		"@types/react-dom": "^18.3.1",
		"bunup": "^0.15.14",
		"typescript": "^5.6.3"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	}
}
