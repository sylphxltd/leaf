{
	"name": "@sylphx/reactpress",
	"version": "0.1.0",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./plugins/routes.js": {
			"import": "./dist/plugins/routes.js",
			"types": "./dist/plugins/routes.d.ts"
		}
	},
	"files": ["dist"],
	"scripts": {
		"build": "bunup 'src/**/*.ts' && node ../../scripts/fix-exports.js",
		"dev": "bunup 'src/**/*.ts' --watch"
	},
	"dependencies": {
		"@mdx-js/react": "^3.1.1",
		"@mdx-js/rollup": "^3.1.1",
		"@vitejs/plugin-react": "^4.3.3",
		"fast-glob": "^3.3.2",
		"gray-matter": "^4.0.3",
		"rehype-highlight": "^7.0.1",
		"rehype-slug": "^6.0.0",
		"remark-gfm": "^4.0.0",
		"vite": "^6.0.1"
	},
	"devDependencies": {
		"@types/node": "^22.10.1",
		"bunup": "^0.15.14",
		"typescript": "^5.6.3"
	},
	"peerDependencies": {
		"react": "^18.0.0",
		"react-dom": "^19.2.0"
	}
}
